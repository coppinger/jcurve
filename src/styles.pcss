@tailwind base;
@tailwind components;
@tailwind utilities;

@font-face {
	font-family: 'Mabry';
	src: url('/mabry.woff2') format('opentype');
	font-weight: normal;
	font-style: normal;
}

.gr-border {
	@apply border rounded overflow-hidden;
}

.sm-divider {
	@apply h-8 border-l w-1;
}

.lg-divider {
	@apply h-48 border-l w-1;
}
